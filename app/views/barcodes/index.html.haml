.pull-right
  =link_to "Import Barcodes", new_barcode_path, :class => "btn btn-default"
  =link_to "Generate New Barcodes", generate_barcodes_path, method: :post, :class => "btn btn-default"

%h1
  Barcodes

%table.table.table-striped
  %tbody
    %tr
      %th Id
      %th Barcode
      %th Source
    - @barcodes.each do |barcode|
      %tr
        %td= barcode.id
        %td= barcode.code
        %td= barcode.source
